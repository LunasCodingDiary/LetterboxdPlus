{
  "name": "letterboxdplus",
  "version": "1.0.0",
  "description": "Film Review site to keep track of what you have watched and wanna watch",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:dev": "npm run build:dev & npm run start:server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start:server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "start-server": "set JWT=shh& set SEED=true& set LOGGING=true& set PASS=fullstack25& set USER=postgres& nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "keywords": [],
  "author": "Luna",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.14"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "axios": "^0.24.0",
    "babel": "^6.23.0",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "css-loader": "^6.5.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "history": "^5.1.0",
    "inflection": "^1.13.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.3",
    "morgan": "^1.10.0",
    "npm-check-updates": "^11.8.5",
    "numeral": "^2.0.6",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-thunk": "^2.4.0",
    "sequelize": "^6.9.0",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
